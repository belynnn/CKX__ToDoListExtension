{
  "manifest_version": 1,
  "name": "TODO list",
  "version": "1.0.0",
  "description": "A browser extension to always have a to do list in hands !",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./images/icon-16.png",
      "32": "./images/icon-32.png",
      "48": "./images/icon-48.png",
      "128": "./images/icon-128.png"
    }
  },
  "icons": {
    "16": "./images/icon-16.png",
    "32": "./images/icon-32.png",
    "48": "./images/icon-48.png",
    "128": "./images/icon-128.png"
  }
}